<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.upv.pepelauraeloy.apptelem">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Estadisticas" />
        <activity android:name=".Predictiva" />
        <activity android:name=".OfertaAsignaturas" />
        <activity
            android:name=".DetailAsignatura"
            android:label="@string/title_activity_detail_asignatura"
            android:parentActivityName=".OfertaAsignaturas"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="es.upv.pepelauraeloy.apptelem.OfertaAsignaturas" />
        </activity>
        <activity android:name=".Plan" />
        <activity android:name=".AddOptativo"></activity>
    </application>

</manifest>